<% unless current_user.current_user? object[:user] %>
  <div id="follow_form">
  <% if current_user.following? object[:user] %>
    <%= render "unfollow",
      object: {relationship_destroy: object[:relationship_destroy],
        user: object[:user]} %>
  <% else %>
    <%= render "follow",
      object: {relationship_build: object[:relationship_build],
        user: object[:user]} %>
  <% end %>
  </div>
<% end %>
